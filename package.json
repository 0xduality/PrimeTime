{
  "name": "PrimeTime",
  "description": "Prime Time NFT",
  "author": "0xduality",
  "license": "Affero General Public License",
  "version": "1.0.0",
  "files": [
      "src/**/*.sol"
  ],
  "scripts": {
      "setup": "forge clean && forge build",
      "update": "forge update",
      "test": "forge clean && forge test",
      "cov": "forge clean && forge coverage --via-ir --report lcov && genhtml -o report lcov.info",
      "snapshot": "forge clean && forge snapshot",
      "gas-report": "forge clean && forge test --gas-report",
      "deploy": ". `pwd`/.env && echo $RPC_URL && forge script Deploy --rpc-url $RPC_URL --broadcast -vvvv --private-key $DEPLOYER_KEY", 
      "mint":  ". `pwd`/.env && cast send 0xE7bA07A767ebDeAaf65c5148646e3aD7d79eF6ba 'mint()' --gas-limit 600000 --value '0.1 ether' --rpc-url=$RPC_URL --private-key=$DEPLOYER_KEY", 
      "see":  ". `pwd`/.env && cast call 0xE7bA07A767ebDeAaf65c5148646e3aD7d79eF6ba 'tokenURI(uint256)(string)' 0 --rpc-url=$RPC_URL", 
      "mint1":  ". `pwd`/.env && cast send --value '0.1 ether' --rpc-url=$RPC_URL 0x7615aa588432A4c0F3E24349f48C48cB9420Aa49 'mint()' --private-key=$DEPLOYER_KEY", 
      "withdraw":  ". `pwd`/.env && cast send --rpc-url=$RPC_URL 0x7615aa588432A4c0F3E24349f48C48cB9420Aa49 'withdraw()' --private-key=$DEPLOYER_KEY", 
  }
}
